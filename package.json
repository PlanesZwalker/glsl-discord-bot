{
  "name": "shaderbot",
  "version": "2.1.0",
  "description": "ShaderBot - Compilateur de shaders GLSL/WGSL professionnel pour Discord. Compilez des shaders personnalisés, explorez 100+ shaders prédéfinis, ou générez des shaders sans coder. Le tout avec de magnifiques GIFs animés, tableau de bord web et API REST.",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "bot": "node bot.js",
    "dev": "nodemon bot.js",
    "dev:bot": "nodemon bot.js",
    "build": "echo 'Build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage": "node scripts/get-coverage.js",
    "check-deployments": "node scripts/check-deployments.js",
    "test:local": "node tests/test-local.js",
    "test:rainbow": "node tests/test-local.js rainbow",
    "test:mandelbrot": "node tests/test-local.js mandelbrot",
    "test:plasma": "node tests/test-local.js plasma",
    "test:animate:spiral": "node tests/test-animate-spiral.js",
    "test:all": "node tests/test-all-commands.js",
    "test:compilation": "node tests/test-compilation-complete.js"
  },
  "keywords": [
    "discord",
    "bot",
    "glsl",
    "shader",
    "webgl",
    "render",
    "production"
  ],
  "author": "Assistant IA Claude",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "gif-encoder-2": "^1.0.5",
    "gifencoder": "^2.0.1",
    "ioredis": "^5.3.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pngjs": "^7.0.0",
    "puppeteer": "^24.29.1",
    "sharp": "^0.33.2",
    "sqlite3": "^5.1.6",
    "stripe": "^14.21.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PlanesZwalker/glsl-discord-bot.git"
  },
  "bugs": {
