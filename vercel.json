{
  "version": 2,
  "builds": [
    {
      "src": "api/discord-ultra.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/security.txt",
      "dest": "/public/security.txt"
    },
    {
      "src": "/discord",
      "dest": "/api/discord-ultra.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, X-Signature-Ed25519, X-Signature-Timestamp"
      }
    },
    {
      "src": "/discord-ultra",
      "dest": "/api/discord-ultra.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, X-Signature-Ed25519, X-Signature-Timestamp"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/discord-ultra.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
