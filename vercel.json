{
  "version": 2,
  "builds": [
    {
      "src": "api/discord-simple.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/security.txt",
      "dest": "/public/security.txt"
    },
    {
      "src": "/discord",
      "dest": "/api/discord-simple.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/discord-public",
      "dest": "/api/discord-simple.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/discord-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
